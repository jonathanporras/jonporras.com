<script type="text/javascript">
  function loadTracking () {
    trackPageView();
    trackBuyButtons();
  }

  function trackBuyButtons () {
    var buyButtons = document.querySelectorAll('.tokonoma-page-buy-button');
    buyButtons.forEach(function (buyButton) {
      buyButton.onclick = function () {
        window.mixpanel.track('Buy Button Clicked - ' + this.dataset.buttonType)
      }
    });
  }

  function trackPageView () {
    window.mixpanel.track('Tokonoma Landing Page View');
    var media = document.getElementsByTagName("audio")[0];
    var mediaTwo = document.getElementsByTagName("audio")[1];
    media.addEventListener('playing', function() {
      window.mixpanel.track('Generative Counterpoint Sound Sample Play');
      mediaTwo.pause();
    });
    mediaTwo.addEventListener('playing', function() {
      window.mixpanel.track('Stochastic Return Sound Sample Play');
      media.pause();
    });
  }
  if (window.addEventListener) {
    // W3C standard
    window.addEventListener('load', loadTracking, false); // NB **not** 'onload'
  } else if (window.attachEvent) {
    // Microsoft
    window.attachEvent('onload', loadTracking);
  }
</script>