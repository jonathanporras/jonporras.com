<div class="row">
  <div class="documents-section-wrapper">
    <div class="col-xs-12">
      <% for(var i=0; i<documentData.length; i++) { %>
        <div class="document-wrapper">
          <div class="row">
            <div class="document-image-wrapper col-xs-12 col-sm-6">
              <a href="/document/<%= documentData[i].slug %>" class="document-link">
                <img src="<%= documentData[i].imgSrc %>">
              </a>
            </div>
            <div class="document-text-wrapper col-xs-12 col-sm-6">
              <a href="/document/<%= documentData[i].slug %>" class="document-link">
                <h2 class="document-header"><%= documentData[i].documentHeader %></h2>
              </a>
              <p class="document-sub-header"><%= documentData[i].date %></p>
              <a href="documents/<%= documentData[i].category %>" class="document-link">
                <p class="document-sub-header"><%= documentData[i].category %></p>
              </a>
            </div>
          </div>  
        </div>
      <% } %>
    </div>
  </div>  
</div>