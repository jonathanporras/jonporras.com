<div class="container">
	<div class="row splash-page">
		<div class="landing-wrapper col-xs-12">
			<a href="/music">
				<div class="title-wrapper col-xs-12">
					<svg width="335" height="160">
						<pattern id="pattern" patternUnits="userSpaceOnUse"
			             width="200" height="300" viewbox="0 0 200 300">
						 <image xlink:href="images/jp-sky.jpg" width="200" height="300" />
						</pattern>
		        		<text style="fill: url(#pattern);" x="0" y="150">Jon Porras</text>
		      		</svg>
	      		</div>
      		</a>
			<ul>
				<!-- <a href="/music"><li class="link">Music</li></a>
				<a href="/webdevelopment"><li>Web Development</li></a> -->
			</ul>
		</div>
		<div class="quote-wrapper col-xs-12">
		</div>
	</div>
	<div class="row">
		<div class="documents-section-wrapper">
			<div class="col-xs-12">
				<% for(var i=0; i<documentData.length; i++) { %>
					<div class="document-wrapper">
						<div class="row">
							<div class="document-image-wrapper col-xs-12 col-sm-6">
								<img src="<%= documentData[i].imgSrc %>">
							</div>
							<div class="document-text-wrapper col-xs-12 col-sm-6">
								<h2 class="document-header"><%= documentData[i].documentHeader %></h2>
								<div class="document-body"><%= documentData[i].documentBody %></div>
							</div>
						</div>	
					</div>
				<% } %>
			</div>
		</div>	
	</div>
</div>